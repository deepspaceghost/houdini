#!/home/deepspaceghost/dev/houdini/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Houdini','console_scripts','houdini'
__requires__ = 'Houdini'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Houdini', 'console_scripts', 'houdini')()
    )
